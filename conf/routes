GET         /                    controllers.HomeController.index()

GET         /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)

GET         /show/:id            controllers.HomeController.show(id:Int)

GET         /add                 controllers.HomeController.add()
GET         /create              controllers.HomeController.create()

GET         /edit                controllers.HomeController.edit()
POST        /editB               controllers.HomeController.editB()

GET         /edit2/:id           controllers.HomeController.edit2(id:Int)
POST        /update/:id          controllers.HomeController.update(id:Int)

GET         /delete              controllers.HomeController.delete()
POST        /deleteB             controllers.HomeController.deleteB()

GET         /delete2             controllers.HomeController.delete2(id:Int)
POST        /remove/:id          controllers.HomeController.remove(id:Int)